{
  "version": 2,
  "name": "floatingdots",
  "alias": "floatingdots.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "useBuildUtils": "@now/build-utils"
      }
    }
  ],
  "regions": ["all"],
  "public": false,
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public, immutable, s-maxage=31536000"
      },
      "dest": "/_nuxt/$1"
    },
    {
      "src": "/icon/(.*)",
      "headers": {
        "cache-control": "public, immutable, s-maxage=31536000"
      },
      "dest": "/icon/$1"
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "cache-control": "public, immutable, s-maxage=31536000"
      },
      "dest": "/favicon.ico"
    },
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "no-cache, no-store, max-age=0, s-maxage=0"
      },
      "dest": "/sw.js"
    }
  ]
}
