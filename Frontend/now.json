{
  "version": 2,
  "name": "floatingdots",
  "alias": "floatingdots.com",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "useBuildUtils": "@now/build-utils"
      }
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/static/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/icon/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "sw.js",
      "headers": {
        "cache-control": "no-cache, no-store"
      }
    }
  ]
}
